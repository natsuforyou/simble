<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	   xsi:schemaLocation=
			   "http://www.springframework.org/schema/beans		http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
				http://code.alibabatech.com/schema/dubbo        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="${app.name}" owner="Rick.Qian" logger="slf4j"/>
    
    <dubbo:registry protocol="zookeeper" address="${connectionString}" file="/data/programData/dubbo/cache/${app.name}.cache"/>
    
    <dubbo:consumer timeout="60000" retries="0" check="false"/>

	<dubbo:annotation package="com.rick.qian.front.controller"/>

</beans>
